{
  "name": "elwins-test-web-components-react",
  "sideEffects": false,
  "version": "0.1.0",
  "description": "React specific wrapper for my test Web Components library",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/elwinvaneede/elwins-test-web-components-react.git"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf dist",
    "compile": "npm run tsc",
    "tsc": "tsc -p .",
    "rollup": "rollup -c"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@types/jest": "^23.3.14",
    "@types/node": "^13.13.9",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "jest": "^26.0.1",
    "jest-dom": "^3.0.2",
    "np": "^6.2.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "elwins-test-web-components": "^0.1.0"
  },
  "peerDependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  }
}
